                        android:layout_height="wrap_content"
                        android:text="Timed"
                        android:id="@+id/textTimed"
                        android:layout_alignTop="@+id/Timed"
                        android:layout_alignBottom="@+id/Timed"
                        android:layout_alignLeft="@+id/Timed"
                        android:layout_alignRight="@+id/Timed"
                        android:gravity="center"
                        android:textColor="#ff000000"
                        android:textSize="18sp"
                        android:layout_marginTop="10sp" />

                    <TextView
